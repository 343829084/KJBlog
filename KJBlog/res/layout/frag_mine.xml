<?xml version="1.0" encoding="utf-8"?>
<org.kymjs.kjframe.widget.KJScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/mine_title_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/mine_tv_collect"
                style="@style/option_item_rl"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:drawableTop="@drawable/ic_launcher"
                android:gravity="center"
                android:text="@string/my_collect" />

            <TextView
                android:id="@+id/mine_tv_join"
                style="@style/option_item_rl"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:drawableTop="@drawable/ic_launcher"
                android:gravity="center"
                android:text="@string/join_qgroup" />

            <TextView
                android:id="@+id/mine_tv_other"
                style="@style/option_item_rl"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:drawableTop="@drawable/ic_launcher"
                android:gravity="center"
                android:text="哈哈" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/mine_rl_push"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="15dp" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/mine_rl_about"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="15dp" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/mine_rl_clean"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="15dp" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/mine_rl_feedback"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="15dp" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/mine_rl_exit"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="15dp" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <TextView
            android:id="@+id/mine_tv_currentversion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:padding="20dp" />
    </LinearLayout>

</org.kymjs.kjframe.widget.KJScrollView>